{
  "name": "hocking-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "shared"
  ],
  "scripts": {
    "start": "npm run start --workspace=server",
    "dev": "npm run dev --workspace=server",
    "build": "npm run build --workspace=server",
    "install:all": "npm install && npm install --workspace=server && npm install --workspace=shared"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
